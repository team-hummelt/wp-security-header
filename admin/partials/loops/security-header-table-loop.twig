{% for d in data %}
    <form method="post" class="security-header-formular-data needs-validation">
        <input type="hidden" name="method" value="security-header-handle">
        <input type="hidden" name="handle" value="{{ d.id }}">
        <div class="card shadow-sm mb-3">
            <div class="card-header py-3 fs-5">
                {{ d.bezeichnung }}
            </div>
            <div class="card-body">
                <div id="{{ d.id }}">
                    {{ include('@partials-loops/security-header-table.twig') }}
                </div>
                <button data-type="add-header-config" data-handle="{{ d.id }}"
                        type="button" class="sh-plugin-action btn btn-success btn-sm mt-3">
                    <i class="bi bi-node-plus me-1"></i>
                    {{ 'add new value'|__ }}
                </button>
                <hr>
                <button type="submit" class="btn btn-blue">
                    <i class="bi bi-save2 me-1"></i>
                    <span class="fw-light">{{ d.bezeichnung }}</span> {{ 'Save'|__ }}
                </button>
            </div>
        </div>
    </form>
{% endfor %}



